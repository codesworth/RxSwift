# Uncomment the next line to define a global platform for your project

platform :ios, '9.0'
use_modular_headers!
workspace 'RX-SERIES'

target 'RX-SERIES' do
project 'RX-SERIES.xcodeproj'
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  
  #pods for RxGram
  pod 'RxSwift',    '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'
end

target 'RxGram' do
project 'RxGram/RxGram.xcodeproj'
  # Pods for RX-SERIES
    pod 'RxSwift',    '~> 4.0'
    pod 'RxCocoa',    '~> 4.0'
end
